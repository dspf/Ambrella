@{
    ViewData["Title"] = "Home Page";
}
<!DOCTYPE html>
<html>
<head>
    <title>@ViewData["Title"]</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <style>
        /* Add your CSS styles here */
        .carousel-item {
            height: 100vh;
            min-height: 350px;
            background: no-repeat center center scroll;
            -webkit-background-size: cover;
            -moz-background-size: cover;
            -o-background-size: cover;
            background-size: cover;
        }

        .carousel-caption {
            top: 50%;
            transform: translateY(-50%);
            text-shadow: 1px 1px 3px rgba(0, 0, 0, 0.5);
        }

        .fade-in {
            animation: fadeIn 2s;
        }

        @@keyframes fadeIn {
            from {
                opacity: 0;
            }

            to {
                opacity: 1;
            }
        }

        .service-card {
            transition: transform 0.3s ease-in-out;
        }

        .service-card:hover {
            transform: scale(1.05);
        }

        .testimonial-card {
            background-color: #f8f9fa;
            padding: 20px;
            border-radius: 10px;
            box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
        }

        .testimonial-card img {
            max-width: 80px;
            border-radius: 50%;
        }

        .landlord-profile {
            text-align: center;
            margin-bottom: 30px;
        }

        .landlord-profile img {
            max-width: 150px;
            border-radius: 50%;
            margin-bottom: 10px;
        }
    </style>
</head>
<body>
    <div class="container-fluid p-0">
        <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
            <ol class="carousel-indicators">
                <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="3"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="4"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="5"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="6"></li>




                <!-- Add more carousel indicators as needed -->
            </ol>
            <div class="carousel-inner">
                <div class="carousel-item active" style="background-image: url('https://images.unsplash.com/photo-1612637968894-660373e23b03?q=80&w=1470&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D');">
                    <div class="carousel-caption d-none d-md-block">
                        <h1 class="fade-in">Welcome to Our Accommodation</h1>
                        <p class="fade-in">Experience luxury and comfort in our world-class accommodations.</p>
                    </div>
                </div>
                <!-- Add more carousel items with different background images -->
                <div class="carousel-item" style="background-image: url('https://images.unsplash.com/photo-1457276587196-a9d53d84c58b?q=80&w=1470&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D');">
                    <div class="carousel-caption d-none d-md-block">
                        <h1 class="fade-in">Explore Our Amenities</h1>
                        <p class="fade-in">Indulge in our state-of-the-art facilities and amenities.</p>
                    </div>
                </div>
                <div class="carousel-item" style="background-image: url('https://images.unsplash.com/photo-1619542402915-dcaf30e4e2a1?q=80&w=1470&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D');">
                    <div class="carousel-caption d-none d-md-block">
                        <h1 class="fade-in">Book Your Stay</h1>
                        <p class="fade-in">Reserve your room now and experience the ultimate luxury.</p>
                    </div>
                </div>
                <div class="carousel-item" style="background-image: url('https://lh3.googleusercontent.com/s-rP2EpobJo-pS-73jTAUJcZuTuFMIiK-kzdzQfqFvB_XTMKrB_ePV4qL-LZ8ZLAXtzwJbbQ4vbyteFAbtYH0SauEBnCUuCgLoY=s750');">
                    <div class="carousel-caption d-none d-md-block">
                        <h1 class="fade-in">Our Universities</h1>
                        <p class="fade-in">Reserve your room now and experience the ultimate luxury.</p>
                    </div>
                </div>
                <div class="carousel-item" style="background-image: url('https://www.dut.ac.za/wp-content/uploads/2023/06/DUT-Steve-Biko-campus-2.jpg');">
                    <div class="carousel-caption d-none d-md-block">
                        <h1 class="fade-in">Easy way to connect </h1>
                        <p class="fade-in">Reserve your room now and experience the ultimate luxury.</p>
                    </div>
                </div>
                <div class="carousel-item" style="background-image: url('https://plus.unsplash.com/premium_photo-1682145401270-3b7f501c8844?q=80&w=1470&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D');">
                    <div class="carousel-caption d-none d-md-block">
                        <h1 class="fade-in">Be happy With Us</h1>
                        <p class="fade-in">Reserve your room now and experience the ultimate luxury.</p>
                    </div>
                </div>
                <div class="carousel-item" style="background-image: url('https://plus.unsplash.com/premium_photo-1661755100242-618b5a580db9?q=80&w=1632&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D');">
                    <div class="carousel-caption d-none d-md-block">
                        <h1 class="fade-in">Amnities</h1>
                        <p class="fade-in">Reserve your room now and experience the ultimate luxury.</p>
                    </div>
                </div>
                <!-- Add more carousel items here -->
            </div>
            <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </div>

    <!-- Rest of your content here -->
    <div class="container my-5 bg-dark text-white p-4 hover-scale">
        <div class="row">
            <div class="col-md-8 offset-md-2 text-center">
                <h1 class="display-4">Welcome</h1>
                <p class="lead">Experience the ultimate luxury at our accommodation. Indulge in world-class amenities, exceptional service, and breathtaking surroundings.</p>
            </div>
        </div>
    </div>

    <div class="container my-5 bg-dark text-white p-4 hover-scale">
        <h2 class="text-center mb-4">Our Services</h2>
        <div class="row">
            <div class="col-md-4 mb-4">
                <div class="card service-card bg-dark text-white">
                    <img src="https://example.com/service1.jpg" class="card-img-top" alt="Service 1">
                    <div class="card-body">
                        <h5 class="card-title">Service 1</h5>
                        <p class="card-text">Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
                    </div>
                </div>
            </div>
            <div class="col-md-4 mb-4">
                <div class="card service-card bg-dark text-white">
                    <img src="https://example.com/service2.jpg" class="card-img-top" alt="Service 2">
                    <div class="card-body">
                        <h5 class="card-title">Service 2</h5>
                        <p class="card-text">Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
                    </div>
                </div>
            </div>
            <div class="col-md-4 mb-4">
                <div class="card service-card bg-dark text-white">
                    <img src="https://example.com/service3.jpg" class="card-img-top" alt="Service 3">
                    <div class="card-body">
                        <h5 class="card-title">Service 3</h5>
                        <p class="card-text">Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="container my-5 bg-dark text-white p-4 hover-scale">
        <h2 class="text-center mb-4">Landlord Profiles</h2>
        <div class="row">
            <div class="col-md-4 mb-4">
                <div class="landlord-profile">
                    <img src="https://example.com/landlord1.jpg" alt="Landlord 1">
                    <h5>John Doe</h5>
                    <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
                </div>
            </div>
            <div class="col-md-4 mb-4">
                <div class="landlord-profile">
                    <img src="https://example.com/landlord2.jpg" alt="Landlord 2">
                    <h5>Jane Smith</h5>
                    <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
                </div>
            </div>
            <div class="col-md-4 mb-4">
                <div class="landlord-profile">
                    <img src="https://example.com/landlord3.jpg" alt="Landlord 3">
                    <h5>Michael Johnson</h5>
                    <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
                </div>
            </div>
        </div>
    </div>

    <style>
        .bg-dark {
            background-color: rgba(0, 0, 0, 0.8) !important;
        }

        .hover-scale:hover {
            transform: scale(1.02);
            transition: transform 0.3s ease-in-out;
        }
    </style>
</body>
</html>
